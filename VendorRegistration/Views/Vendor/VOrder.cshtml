@model IEnumerable<VendorRegistration.Models.Order>

@{
    ViewBag.Title = "Order";
}

<h2>Order</h2>

<p>
    @Html.ActionLink("Back to Admin", "AfterLogin", "Admin")
</p>
<table class="table">
    <tr style="color:black">
        <th>
            Order Id
        </th>
        <th>
            Customer Name
        </th>
        <th>
            Newspaper
        </th>
        <th>
            No: of days subscribed
        </th>
        <th>
            invoiceId
        </th>
        <th>
            Order date
        </th>
        <th>
            Price
        </th>
        <th>
            Total Amount
        </th>


        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderId)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.tbl_invoice.Customer.CustomerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Newspaper.NewspaperName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderQty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_invoice.InvoiceId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Order_Unitprice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderBill)
            </td>

        </tr>
    }

</table>

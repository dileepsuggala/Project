@model IEnumerable<VendorRegistration.Models.Feedback>

@{
    ViewBag.Title = "ContactUs";
}

<!DOCTYPE html>
<html>
<body background="~/image/Black.jpg" style="background-repeat:no-repeat;background-size:cover">


    <table class="table">
        <tr style="color:papayawhip;font-size:30px">
            <th >
                @Html.DisplayNameFor(model => model.PhoneNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ftext)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr style="color:darkgrey;font-size:20px">
                <td>
                    @Html.DisplayFor(modelItem => item.PhoneNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ftext)
                </td>

            </tr>
        }

    </table>
    </body>
</html>